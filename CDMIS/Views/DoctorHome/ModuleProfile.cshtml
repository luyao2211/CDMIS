                        $("#QE0003" + i).append(NewElement);
                    }
                }
            });
            $('*[id="MstLipidDrug"]').attr('lang', i);
            i = i + 1;
        }
        i = 1;
        while ($("#QE0004" + i).length > 0) {
            $.ajax({
                url: "/DoctorHome/GetTypeNameList",
