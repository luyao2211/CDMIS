    function IsLegal(o) {
        if (document.getElementById(o).value != "") {
            var re = /^\d+$|^\d+\.\d+$/;
            if (!re.test(document.getElementById(o).value)) {
                $("." + o).html("输入格式有误！");

                $("."+o).show();
            }
            else {
                $("."+o).hide();
