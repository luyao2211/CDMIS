    function ShowOrHide(HeaderId, HeaderStatus) {
        if (HeaderStatus == "1") {
            $("*[lang=" + HeaderId + "]").show();
        }
        else if (HeaderStatus == "+") {
            $("*[lang=" + HeaderId + "]").show();
            $("#" + HeaderId).val("-");
        }
        else if (HeaderStatus == "-") {
            $("*[lang=" + HeaderId + "]").hide();
            $("#" + HeaderId).val("+");
        }
        else {
            $("*[lang=" + HeaderId + "]").hide();
        }
    }

