<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/sticky.full.css")" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="../../Scripts/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="../../Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
    <script type="text/javascript" src="../../Scripts/sticky.full.js"></script>
    <script type="text/javascript" src="../../Scripts/My97DatePicker/WdatePicker.js"></script>
</head>
<body>
    <div id="userOverview">
        @{Html.RenderAction("UserOverview", "Personal");}
    </div>
    <div id="header">
        <h1>
            慢病中心管理系统</h1>
    </div>
    <div id="page">
        <div style="height: 80px">
        </div>
        @{Html.RenderAction("PatientBasicInfo", "PatientInfo");}
        <ul id="menu">
            <li>@Html.ActionLink("健康模块", "PatientDetailInfo", "PatientHome")</li>
            <li>@Html.ActionLink("临床信息", "ClinicInfo", "PatientHome")</li>
            <li>@Html.ActionLink("健康参数", "HealthParameters", "PatientHome")</li>
            <li>@Html.ActionLink("症状管理", "SymptomsManagement", "PatientHome")</li>
            <li>@Html.ActionLink("治疗方案", "TreatmentPlan", "PatientHome")</li>
            <li>@Html.ActionLink("警报记录", "PatientAlert", "PatientHome")</li>
            <li>@Html.ActionLink("每日任务", "EverydayTask", "PatientHome")</li>
        </ul>
        <div id="content">
            @RenderBody()
        </div>
        <div id="footer">
            版权所有
        </div>
    </div>
</body>
</html>
